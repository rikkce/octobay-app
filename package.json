{
  "name": "octobay",
  "description": "OpenQ - Ethereum Payment Service for GitHub Users",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "app:dev": "nuxt",
    "app:generate": "nuxt generate --force-build",
    "app:deploy": "push-dir --dir=dist --branch=gh-pages --cleanup",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,sass}\" --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "@octobay/adapters": "^0.0.8",
    "@primer/octicons": "^12.0.0",
    "bootstrap": "^4.5.2",
    "core-js": "^3.8.3",
    "nuxt": "^2.14.12",
    "v-clipboard": "^2.2.3",
    "v-tooltip": "^2.0.3",
    "vue-moment": "^4.1.0",
    "vuejs-datepicker": "^1.6.2",
    "web3": "^1.3.1"
  },
  "devDependencies": {
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/fontawesome": "^1.1.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.5.0",
    "fibers": "^5.0.0",
    "husky": "^5.0.9",
    "lint-staged": "^10.5.3",
    "pinst": "^2.1.4",
    "prettier": "^2.2.1",
    "push-dir": "^0.4.1",
    "sass": "^1.32.7",
    "sass-loader": "10",
    "stylelint": "^13.9.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0"
  }
}